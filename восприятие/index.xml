    <item>
      <title>Подтекст</title>
      <link>http://localhost:1313/russian_for_life/%D0%B2%D0%BE%D1%81%D0%BF%D1%80%D0%B8%D1%8F%D1%82%D0%B8%D0%B5/%D0%BF%D0%BE%D0%B4%D1%82%D0%B5%D0%BA%D1%81%D1%82/</link>
